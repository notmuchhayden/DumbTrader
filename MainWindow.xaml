<Window x:Class="DumbTrader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DumbTrader"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Hello World" HorizontalAlignment="Center" FontSize="24" Margin="0,0,0,20"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Button x:Name="BtnTestXing" Content="서버접속" Width="120" Height="30" Click="BtnTestXing_Click" Margin="0,0,10,0"/>
                <Button x:Name="BtnLogin" Content="Login" Width="80" Height="30" Click="BtnLogin_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
